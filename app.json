{
  "expo": {
    "name": "Stray Safe",
    "slug": "stray-safe",
    "version": "1.0.0",
    "icon": "./assets/StraySafeLogo.png",
    "scheme": "straysafe",
    "android": {
      "package": "com.example.straysafe",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBCKJ3TRt7zOYtHjac7CVTcOS5xie0FZ4E"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "splash": {
      "image": "./assets/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Stray Safe to use your location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.straysafe",
      "config": {
        "googleMapsApiKey": "AIzaSyBCKJ3TRt7zOYtHjac7CVTcOS5xie0FZ4E"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Stray Safe to use your location."
      }
    },
    "extra": {
      "eas": {
        "projectId": "1b061dfb-1413-49f1-944c-ab3e4e9f90ca"
      }
    }
  }
}
